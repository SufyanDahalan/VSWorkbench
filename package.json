{
	"name": "gitlabcode",
	"displayName": "GitLabCode",
	"description": "",
	"version": "0.0.1",
	"publisher": "SufyanDahalan",
	"engines": {
		"vscode": "^1.66.0"
	},
	"main": "./dist/extension.js",
	"extensionKind": [
		"ui"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"viewsWelcome": [
			{
				"view": "authentication",
				"contents": "No personal access token detected!\nPlease create a personal access token [learn more](https://docs.gitlab.com/ee/user/profile/personal_access_tokens.html#create-a-personal-access-token/).\n[Authenticate](command:GitLabCode.addPersonalAccessToken)"
			},
			{
				"view": "settings",
				"contents": "Update Authentication Information [learn more](https://docs.gitlab.com/ee/user/profile/personal_access_tokens.html#create-a-personal-access-token/).\n[Update Perosnal Authentication Token](command:GitLabCode.updatePersonalAccessToken)"
			}
		],
		"commands": [
			{
				"command": "GitLabCode.helloWorld",
				"title": "Hello World",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.addPersonalAccessToken",
				"title": "Authenticate",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.updatePersonalAccessToken",
				"title": "Update Personal Token",
				"category": "GitLabCode Personal"
			},
			{
				"command": "GitLabCode.createPersonalProject",
				"title": "Create Personal Project",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.getUserGroups",
				"title": "Get User Groups Information",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.createGroupProject",
				"title": "New Group Project",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.createGroup",
				"title": "Create Group",
				"category": "GitLabCode",
				"icon": "$(ports-forward-icon)"
			},
			{
				"command": "GitLabCode.createSubGroup",
				"title": "Create Subgroup",
				"category": "GitLabCode",
				"icon": "$(ports-forward-icon)"
			},
			{
				"command": "GitLabCode.createIssue",
				"title": "Create Issue",
				"category": "GitLabCode",
				"icon": "$(ports-forward-icon)"
			},
			{
				"command": "GitLabCode.createMergeRequest",
				"title": "Create Merge Request",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewIssue",
				"title": "View Issue",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewGitTree",
				"title": "View Git Tree",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.refreshGroupView",
				"title": "Refresh Groups View",
				"category": "GitLabCode",
				"icon": "$(extensions-refresh)"
			},
			{
				"command": "GitLabCode.refreshIssueView",
				"title": "Refresh Issues View",
				"category": "GitLabCode",
				"icon": "$(extensions-refresh)"
			},
			{
				"command": "GitLabCode.refreshPipelineView",
				"title": "Refresh Pipelines View",
				"category": "GitLabCode",
				"icon": "$(extensions-refresh)"
			},
			{
				"command": "GitLabCode.deleteNamespaceNode",
				"title": "Delete Group Or Project",
				"category": "GitLabCode",
				"icon": "$(notebook-delete-cell)"
			},
			{
				"command": "GitLabCode.createPersonalSnippet",
				"title": "Create Personal Snippet",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.createProjectSnippet",
				"title": "Create Project Snippet",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewIssueList",
				"title": "View Issue List",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewIssueBoard",
				"title": "View Issue Board",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.openIssueBoardInGitLab",
				"title": "Open List In GitLab",
				"category": "GitLabCode",
				"icon": "$(globe)"
			},
			{
				"command": "GitLabCode.openListInGitLab",
				"title": "Open Board In GitLab",
				"category": "GitLabCode",
				"icon": "$(globe)"
			},
			{
				"command": "GitLabCode.openSettingsInGitLab",
				"title": "Open Settings In GitLab",
				"category": "GitLabCode",
				"icon": "$(gear)"
			},
			{
				"command": "GitLabCode.addMemberToProject",
				"title": "Add Member To Project",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.addMemberToGroup",
				"title": "Add Member To Group",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.openPipelinesInGitLab",
				"title": "Open Pipelines In GitLab",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewPipelines",
				"title": "View Pipelines",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewPipeline",
				"title": "View Pipeline",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.viewJob",
				"title": "View Job",
				"category": "GitLabCode"
			},
			{
				"command": "GitLabCode.openInGitLab",
				"title": "Open In GitLab",
				"category": "GitLabCode",
				"icon": "$(globe)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gitLabCode-activityBar",
					"title": "GitLabCode",
					"icon": "$(symbol-type-parameter)"
				}
			],
			"panel": [
				{
					"id": "gitlabcode-panel",
					"title": "GitLabCode",
					"icon": "$(symbol-type-parameter)"
				}
			]
		},
		"views": {
			"gitLabCode-activityBar": [
				{
					"id": "authentication",
					"name": "authentication",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "authentication",
					"visibility": "visible",
					"when": "!GitLabCode.authenticated"
				},
				{
					"id": "groupView",
					"name": "Groups",
					"icon": "$(accounts-view-bar-icon)",
					"contextualTitle": "GitLabCode",
					"visibility": "visible",
					"when": "GitLabCode.authenticated"
				},
				{
					"id": "issueView",
					"name": "Issues",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "collapsed",
					"when": "GitLabCode.authenticated"
				},
				{
					"id": "pipelineView",
					"name": "Pipelines",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "collapsed",
					"when": "GitLabCode.authenticated"
				},
				{
					"id": "settings",
					"name": "Settings",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "collapsed",
					"when": "GitLabCode.authenticated",
					"contents": "Update Personal Authentication Token [learn more](https://www.npmjs.com/).\n[Add Dependency](command:GitLabCode.updatePersonalAccessToken)"
				},
				{
					"id": "stars",
					"name": "Starred Projects",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "collapsed",
					"when": "GitLabCode.authenticated",
					"contents": "@feature section for starred projects along with a couple of actions [api](https://docs.gitlab.com/ee/api/projects.html#list-user-projects), [second api](https://docs.gitlab.com/ee/api/projects.html#list-projects-starred-by-a-user)"
				}
			],
			"gitlabcode-panel": [
				{
					"id": "gitlabPipelines",
					"name": "Pipelines",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "collapsed",
					"when": "GitLabCode.authenticated"
				},
				{
					"id": "gitlabGitTree",
					"name": "Git Tree",
					"icon": "./src/assets/dep.svg",
					"contextualTitle": "GitLabCode",
					"visibility": "visible",
					"when": "GitLabCode.authenticated"
				}
			]
		},
		"menus": {
			"GitLabCode.GroupView": [],
			"GitLabCode.PipelineView": [
				{
					"command": "GitLabCode.viewPipeline",
					"group": "inline",
					"when": "view == pipelineView && viewItem == job"
				},
				{
					"command": "GitLabCode.viewJob",
					"group": "inline",
					"when": "view == pipelineView && viewItem == job"
				}
			],
			"GitLabCode.GroupNode": [
				{
					"command": "GitLabCode.createGroupProject",
					"when": "view == groupView && viewItem == group",
					"group": "navigation"
				},
				{
					"command": "GitLabCode.createPersonalProject",
					"when": "view == groupView && viewItem == user",
					"group": "inline"
				},
				{
					"command": "GitLabCode.createPersonalSnippet",
					"when": "view == groupView && viewItem == user"
				},
				{
					"command": "GitLabCode.createProjectSnippet",
					"when": "view == groupView && viewItem == project"
				},
				{
					"command": "GitLabCode.deleteNamespaceNode",
					"when": "viewItem == project || viewItem == group"
				},
				{
					"command": "GitLabCode.addMemberToProject",
					"when": "view == groupView && viewItem == project",
					"comment": "TODO: add icon?"
				},
				{
					"command": "GitLabCode.addMemberToGroup",
					"when": "view == groupView && viewItem == group",
					"comment": "TODO: add icon?"
				}
			],
			"GitLabCode.IssueNode": [
				{
					"command": "GitLabCode.viewIssue",
					"icon": "$(eye)"
				}
			],
			"view/title": [
				{
					"command": "GitLabCode.refreshGroupView",
					"when": "view == groupView",
					"group": "navigation"
				},
				{
					"command": "GitLabCode.refreshIssueView",
					"when": "view == issueView",
					"group": "navigation"
				},
				{
					"command": "GitLabCode.refreshPipelineView",
					"when": "view == pipelineView",
					"group": "navigation"
				},
				{
					"command": "GitLabCode.createGroup",
					"when": "view == groupView",
					"group": "navigation"
				},
				{
					"submenu": "GitLabCode.GroupView",
					"when": "view == groupView"
				},
				{
					"command": "GitLabCode.viewIssueList",
					"when": "view == issueView"
				},
				{
					"command": "GitLabCode.openPipelinesInGitLab",
					"when": "view == pipelineView"
				},
				{
					"command": "GitLabCode.viewPipelines",
					"when": "view == pipelineView"
				},
				{
					"command": "GitLabCode.viewIssueBoard",
					"when": "view == issueView"
				}
			],
			"view/item/context": [
				{
					"submenu": "GitLabCode.GroupNode",
					"group": "inline@10",
					"when": "view == groupView"
				},
				{
					"submenu": "GitLabCode.IssueNode",
					"group": "inline@10",
					"when": "view == issueView"
				},
				{
					"submenu": "GitLabCode.PipelineView",
					"group": "inline@10",
					"when": "view == pipelineView"
				},
				{
					"command": "GitLabCode.createSubGroup",
					"group": "inline@1",
					"when": "view == groupView && viewItem == group"
				},
				{
					"command": "GitLabCode.openPipelinesInGitLab",
					"group": "Open Pipelines In GitLab",
					"when": "view == pipelineView"
				},
				{
					"command": "GitLabCode.openInGitLab",
					"group": "inline@1"
				},
				{
					"command": "GitLabCode.openSettingsInGitLab",
					"group": "inline@2",
					"when": "view == groupView"
				}
			]
		},
		"submenus": [
			{
				"id": "GitLabCode.GroupView",
				"label": "More Actions",
				"icon": "$(more)"
			},
			{
				"id": "GitLabCode.IssueView",
				"label": "More Actions",
				"icon": "$(more)"
			},
			{
				"id": "GitLabCode.GroupNode",
				"label": "More Actions",
				"icon": "$(more)"
			},
			{
				"id": "GitLabCode.PipelineView",
				"label": "More Actions",
				"icon": "$(more)"
			},
			{
				"id": "GitLabCode.IssueNode",
				"label": "More Actions",
				"icon": "$(more)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"install": "vsce package; code --install-extension ./*.vsix"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.5.5",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"axios": "^0.26.1"
	}
}
